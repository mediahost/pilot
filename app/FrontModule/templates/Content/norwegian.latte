	<div class="container pos-rel z3 flex-cl h100">

		<div class="content typo-light w-60 edit-item" data-sandboxid="tmp-44">
		</div>

	</div><!-- /.container -->
	<div class="bg-section bg-cover bg-cc full-wh edit-item" data-bg="{$basePath}/new/images/nor2.jpg"
		 data-sandboxid="tmp-63"
		 style="background-image: url(&quot;{$basePath}/new/images/nor2.jpg&quot;);">
